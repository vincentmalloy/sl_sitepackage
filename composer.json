{
    "name": "simon-lundius/sitepackage",
    "type": "typo3-cms-extension",
    "description": "Basic Sitepackage",
    "license": "GPL-2.0-or-later",
    "require": {
       "typo3/cms-core": "^10.4 || ^11.0"
    },
    "replace": {
       "typo3-ter/my-extension": "self.version"
    },
    "autoload": {
       "psr-4": {
          "SimonLundius\\SitePackage\\": "Classes/"
       }
    },
    "extra": {
       "typo3/cms": {
          "extension-key": "sl_sitepackage"
       }
    }
 }